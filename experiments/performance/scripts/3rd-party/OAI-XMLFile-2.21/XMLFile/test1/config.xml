<?xml version="1.0" ?>

<xmlfile xmlns="http://simba.cs.uct.ac.za/projects/xmlfile"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://simba.cs.uct.ac.za/projects/xmlfile
                         http://simba.cs.uct.ac.za/projects/xmlfile/xmlfile.xsd"
>

   <repositoryName>test1 OAI Archive</repositoryName>
   <adminEmail>hussein@cs.uct.ac.za</adminEmail>
   <archiveId>test1</archiveId>
   <recordlimit>500</recordlimit>
   <datadir>data</datadir>
   <longids>no</longids>
   <filematch>[^\.]+\.[xX][mM][lL]$</filematch>
   <multiset>no</multiset>
   <originalids>no</originalids>
   <stripextensions>yes</stripextensions>
   <listsize>no</listsize>
   <metadata>
      <prefix>oai_dc</prefix>
      <namespace>http://www.openarchives.org/OAI/2.0/oai_dc/</namespace>
      <schema>http://www.openarchives.org/OAI/2.0/oai_dc.xsd</schema>
      <transform>/usr/bin/xsltproc dc.xsl - |</transform>
   </metadata>
   <metadata>
      <prefix>oai_vracore</prefix>
      <namespace>http://www.gsd.harvard.edu/~staffaw3/vra/vracore3.htm</namespace>
      <schema>http://oai.dlib.vt.edu/OAI/1.1/oai_vracore.xsd</schema>
      <transform>/usr/bin/xsltproc vra.xsl - |</transform>
   </metadata>
</xmlfile>
