<?xml version="1.0" ?>

<xmlfile xmlns="http://simba.cs.uct.ac.za/projects/xmlfile"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://simba.cs.uct.ac.za/projects/xmlfile
                         http://simba.cs.uct.ac.za/projects/xmlfile.xsd"
>

   <repositoryName>test4 OAI Archive</repositoryName>
   <adminEmail>hussein@cs.uct.ac.za</adminEmail>
   <archiveId>test4</archiveId>
   <recordlimit>500</recordlimit>
   <datadir>data</datadir>
   <longids>no</longids>
   <filematch>[^\.]+\.[xX][mM][lL]$</filematch>
   <multiset>no</multiset>
   <originalids>no</originalids>
   <listsize>no</listsize>
   <metadata>
      <prefix>oai_dc</prefix>
      <namespace>http://www.openarchives.org/OAI/2.0/oai_dc/</namespace>
      <schema>http://www.openarchives.org/OAI/2.0/oai_dc.xsd</schema>
      <transform>/usr/bin/xsltproc 2dc.xsl - |</transform>
   </metadata>
   <metadata>
      <prefix>oai_rfc1807</prefix>
      <namespace>http://info.internet.isi.edu:80/in-notes/rfc/files/rfc1807.txt</namespace>
      <schema>http://www.openarchives.org/OAI/1.1/rfc1807.xsd</schema>
      <transform>/usr/bin/xsltproc 2rfc1807.xsl - |</transform>
   </metadata>
</xmlfile>
